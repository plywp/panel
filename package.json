	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check --write . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"auth:generate": "bun x @better-auth/cli generate --config=./src/lib/auth.ts --output=./src/lib/server/db/schema/auth-schema.ts --yes",
		"auth:migrate": "npm run db:generate && npm run db:migrate",
		"shadcn:add": "npx shadcn-svelte@latest add"
	},
